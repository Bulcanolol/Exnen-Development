    write[title + color.Reset];
    var options: $<"<1> Calculate", "<2> Random", "<3> Get Brackets">$
    options.forEach[x :> (
        write[x];
    )]
    var qst: input["Select an option: "];
    if[qst :: 1] (
        write[calc.math[Int.Convert[input["Calculate: "]]]]
    )
    if[qst :: 2] (
